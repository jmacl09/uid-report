      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/favicon.ico",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/api/uid",
      "allowedRoles": [
        "authenticated"
      ],
      "methods": [
